{
  "name": "gmail-backup",
  "version": "0.2.2",
  "description": "gmail backup tool",
  "author": "Ugly Donkey",
  "bin": {
    "gmail-backup": "./bin/run"
  },
  "homepage": "https://github.com/UglyDonkey/gmail-backup",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "UglyDonkey/gmail-backup",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2.3.1",
    "@oclif/plugin-help": "5.2.6",
    "dayjs": "^1.11.5",
    "file-lines-stream": "0.2.0",
    "googleapis": "^105.0.0"
  },
  "devDependencies": {
    "@oclif/test": "2.3.8",
    "@types/chai": "^4.3.0",
    "@types/chai-spies": "^1.0.3",
    "@types/mocha": "^9.1.1",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^18.14.2",
    "chai": "^4.3.4",
    "chai-spies": "^1.0.0",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "mocha": "^9.2.2",
    "mock-fs": "^5.1.4",
    "oclif": "3.7.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4"
  },
  "oclif": {
    "bin": "gmail-backup",
    "dirname": "gmail-backup",
    "commands": "./dist/commands",
    "hooks": {
      "prerun": "./dist/hooks/prerun"
    },
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "scripts": {
    "start:dev": "node bin/dev",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && yarn run readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "yarn run readme && git add README.md",
    "readme": "oclif readme && sed -i s:dist/commands/:src/commands/:g README.md"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": "https://github.com/UglyDonkey/gmail-backup/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
